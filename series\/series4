/* Sample Input-Enter the value of n=10
Sample Output- 1 -3 5 -7 9*/ 


#include<stdio.h>
int main()
{
    int i,c=1,n;
    printf("Enter the value of n=");
    scanf("%d",&n);
    for(i=1;i<=n;i+=2)
    {
        if(c%2==0)
        {
            printf("%d ",(-i));
        }
        else
        {
            printf("%d ",i);
        }
        c=c+1;

    }
    return 0;
}
