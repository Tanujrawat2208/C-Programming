/* Sample Input-Enter the value of n=10
Sample Output-1 10 101 1010 10101 101010 1010101 10101010 101010101 1010101010 */ 


#include<stdio.h>
int main()
{
    int i,c=0,n;
    printf("Enter the value of n=");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(i%2!=0)
        {
            c=c*10+1;
            printf("%d ",c);
        }
        else
        {
            c=c*10;
            printf("%d ",c);
        }
    }
    return 0;
}
