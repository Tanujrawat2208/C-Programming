/* Sample Input-Enter the value of n=10
Sample Output- 1 -3 4 -5 6 -7 8 -9 10 -11  */ 


#include<stdio.h>
int main()
{
    int i,c=1,n;
    printf("Enter the value of n=");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(i%2==0)
        {
            printf("%d ",(-c));
        }
        else
        {
            printf("%d ",c);
        }
        c=i+2;

    }
    return 0;
}
